{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "rootDir": "src",
    "outDir": "build",
    "allowJs": true,
    "target": "ES2020"
  },
  "include": ["src/**/*"]
}
