    "posttest": "yarn run check",
    "build-container": "gcloud builds submit --tag gcr.io/cloud-debugging/egyed-foodvendor",
    "deploy": "gcloud run deploy --image gcr.io/cloud-debugging/egyed-foodvendor --platform managed"
